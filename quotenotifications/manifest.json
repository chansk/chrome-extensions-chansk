{
    "name": "Click to show an alert",
    "description": "A Chrome Extension that shows random quotes as notification",
    "version": "1.0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_icon": {
            "16": "readingImages/icon-16.png",
            "48": "readingImages/icon-48.png",
            "128": "readingImages/icon-128.png"
        }
    },
    "icons": {
        "16": "readingImages/icon-16.png",
        "48": "readingImages/icon-48.png",
        "128": "readingImages/icon-128.png"   
    }
}